{
    "features": {
        "appTrackerProtection": {
            "state": "enabled"
        },
        "contentBlocking": {
            "exceptions": [
            ]
        },
        "eme": {
            "state": "enabled",
            "exceptions": [
                {
                    "domain": "open.spotify.com",
                    "reason": "Site displays a 'Playback disabled' notice on some media, preventing listening."
                }
            ]
        },
        "ampLinks": {
            "state": "enabled"
        },
        "trackingParameters": {
            "state": "enabled",
            "minSupportedVersion": 51250000
        },
        "autoconsent": {
            "state": "enabled"
        },
        "autofill": {
            "state": "enabled"
        },
        "customUserAgent": {
            "state": "enabled",
            "settings": {
                "omitApplicationSites": [
                    {
                        "domain": "mail.google.com",
                        "reason": "Page loads blank after login (inbox does not show)."
                    },
                    {
                        "domain": "vegas.williamhill.com",
                        "reason": "Cannot interact with cookie prompt and some page elements don't load."
                    },
                    {
                        "domain": "homedepot.ca",
                        "reason": "https://github.com/duckduckgo/privacy-configuration/issues/554"
                    }
                ],
                "omitVersionSites": [
                    {
                        "domain": "whitepages.com",
                        "reason": "After performing a search, the site displays a page stating that the browser is not supported, and results are not shown."
                    }
                ]
            }
        },
        "cookie": {
            "state": "enabled",
            "settings": {
                "firstPartyCookiePolicy": {
                    "threshold": 604800,
                    "maxAge": 604800
                }
            },
            "exceptions": [ ]
        },
        "fingerprintingBattery": {
            "state": "enabled"
        },
        "fingerprintingCanvas": {
            "state": "enabled"
        },
        "fingerprintingHardware": {
            "state": "enabled",
            "settings": {
                "keyboard": {"type": "undefined"},
                "hardwareConcurrency": {"type": "number", "value": 8},
                "deviceMemory": {"type": "number", "value": 4}
            }
        },
        "fingerprintingScreenSize": {
            "state": "enabled"
        },
        "fingerprintingTemporaryStorage": {
            "state": "enabled"
        }
    },
    "unprotectedTemporary": [

    ]
}
