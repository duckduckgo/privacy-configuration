{
    "_meta": {
        "description": "Detect web patterns such as adwalls. Supports automatic detection with telemetry.",
        "sampleExcludeRecords": {
            "domain": "example.com",
            "reason": "site breakage"
        },
        "featureFlags": {
            "adwallAutoDetection": "Enable/disable automatic adwall detection",
            "adwallTelemetryPixel": "Enable/disable daily/weekly telemetry pixels",
            "adwallZeroCountPixel": "Enable/disable firing pixels when count is zero"
        }
    },
    "state": "disabled",
    "exceptions": [],
    "settings": {
        "detectors": {
            "adwalls": {
                "generic_en": {
                    "state": "enabled",
                    "match": {
                        "text": {
                            "pattern": [
                                "ad ?block(er)? detected",
                                "adjust(ing)?( your)? ad blocking settings",
                                "(turn off|disable|disabling|deactivate)( your| my)? ad ?block(er)?",
                                "your ad ?blocker is on",
                                "you('re| are| may be) using an ad blocker"
                            ]
                        }
                    },
                    "triggers": {
                        "auto": {
                            "state": "enabled",
                            "intervalMs": [100, 500, 1000],
                            "runConditions": [
                                { "context": { "top": true } }
                            ]
                        },
                        "breakageReport": {
                            "state": "enabled"
                        }
                    },
                    "actions": {
                        "fireTelemetry": {
                            "state": "enabled",
                            "type": "adwall"
                        },
                        "breakageReportData": {
                            "state": "enabled"
                        }
                    }
                },
                "generic_de": {
                    "state": "enabled",
                    "match": {
                        "text": {
                            "pattern": [
                                "deaktivieren sie ihren adblocker",
                                "deaktivieren sie ihre blocker",
                                "adblocker zu deaktivieren",
                                "adblocker deaktivieren",
                                "sie einen adblocker verwenden"
                            ]
                        }
                    },
                    "triggers": {
                        "auto": {
                            "state": "enabled",
                            "intervalMs": [100, 500, 1000],
                            "runConditions": [
                                {
                                    "context": { "top": true },
                                    "urlPattern": "https://*.de/*"
                                }
                            ]
                        },
                        "breakageReport": {
                            "state": "enabled"
                        }
                    },
                    "actions": {
                        "fireTelemetry": {
                            "state": "enabled",
                            "type": "adwall"
                        },
                        "breakageReportData": {
                            "state": "enabled"
                        }
                    }
                },
                "generic_it": {
                    "state": "enabled",
                    "match": {
                        "text": {
                            "pattern": [
                                "disattiva l'ad block",
                                "stai usando un ad block"
                            ]
                        }
                    },
                    "triggers": {
                        "auto": {
                            "state": "enabled",
                            "intervalMs": [100, 500, 1000],
                            "runConditions": [
                                {
                                    "context": { "top": true },
                                    "urlPattern": "https://*.it/*"
                                }
                            ]
                        },
                        "breakageReport": {
                            "state": "enabled"
                        }
                    },
                    "actions": {
                        "fireTelemetry": {
                            "state": "enabled",
                            "type": "adwall"
                        },
                        "breakageReportData": {
                            "state": "enabled"
                        }
                    }
                },
                "generic_se": {
                    "state": "enabled",
                    "match": {
                        "text": {
                            "pattern": [
                                "har du en annonsblockerare igång",
                                "avaktivera din annonsblockerare"
                            ]
                        }
                    },
                    "triggers": {
                        "auto": {
                            "state": "enabled",
                            "intervalMs": [100, 500, 1000],
                            "runConditions": [
                                {
                                    "context": { "top": true },
                                    "urlPattern": "https://*.se/*"
                                }
                            ]
                        },
                        "breakageReport": {
                            "state": "enabled"
                        }
                    },
                    "actions": {
                        "fireTelemetry": {
                            "state": "enabled",
                            "type": "adwall"
                        },
                        "breakageReportData": {
                            "state": "enabled"
                        }
                    }
                },
                "generic_fr": {
                    "state": "enabled",
                    "match": {
                        "text": {
                            "pattern": [
                                "bloc ?publicitaire détecté",
                                "désactivez votre bloqueur de publicités",
                                "vous utilisez un bloqueur de publicités"
                            ]
                        }
                    },
                    "triggers": {
                        "auto": {
                            "state": "enabled",
                            "intervalMs": [100, 500, 1000],
                            "runConditions": [
                                {
                                    "context": { "top": true },
                                    "urlPattern": "https://*.fr/*"
                                }
                            ]
                        },
                        "breakageReport": {
                            "state": "enabled"
                        }
                    },
                    "actions": {
                        "fireTelemetry": {
                            "state": "enabled",
                            "type": "adwall"
                        },
                        "breakageReportData": {
                            "state": "enabled"
                        }
                    }
                }
            }
        }
    }
}
