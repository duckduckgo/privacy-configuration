{
    "_meta": {
        "description": "Protection from canvas API fingerprinting."
    },
    "settings": {
        "webGl": "disabled",
        "additionalEnabledCheck": "enabled",
        "safeMethods": ["putImageData", "drawImage"],
        "unsafeMethods": [
            "strokeRect",
            "bezierCurveTo",
            "quadraticCurveTo",
            "arcTo",
            "ellipse",
            "rect",
            "fill",
            "stroke",
            "lineTo",
            "beginPath",
            "closePath",
            "arc",
            "fillText",
            "fillRect",
            "strokeText",
            "createConicGradient",
            "createLinearGradient",
            "createRadialGradient",
            "createPattern"
        ],
        "unsafeGlMethods": [
            "commit",
            "compileShader",
            "shaderSource",
            "attachShader",
            "createProgram",
            "linkProgram",
            "drawElements",
            "drawArrays"
        ],
        "canvasMethods": ["toDataURL", "toBlob"]
    },
    "exceptions": []
}
