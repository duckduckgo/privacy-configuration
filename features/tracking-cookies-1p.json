{
    "_meta": {
        "description": "1st party cookie expiration protection. Caps 1st party cookies to expire in 7 days.",
        "sampleExcludeRecords": {
            "domain": "example.com",
            "reason": "site breakage"
        }
    },
    "settings": {
        "firstPartyTrackerCookiePolicy": {
            "threshold": 86400,
            "maxAge": 86400
        }
    },
    "exceptions": [
        {
            "domain": "nespresso.com",
            "reason": "login issues"
        }
    ]
}
