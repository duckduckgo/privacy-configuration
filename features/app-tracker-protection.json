{
    "_meta": {
        "description": "App Tracking Protection"
    },
    "settings": {
        "protectGames": {
            "state": "enabled"
        },
        "connectivityChecks": {
            "state": "enabled"
        },
        "ipv6Support": {
            "state": "enabled"
        },
        "privateDnsSupport": {
            "state": "enabled"
        },
        "badHealthMitigation": {
            "state": "enabled",
            "settings": {
                "triggers": {
                    "socketReadExceptionAlerts": {
                        "state": "enabled",
                        "threshold": 20
                    }
                }
            }
        },
        "cpuMonitoring": {
            "state": "enabled"
        }, 
        "exceptionLists": {
            "appTrackerAllowList": [
                {
                    "packageName": "se.prisjakt.pricespy",
                    "domains": [
                        {
                            "domain": "acdn.adnxs.com",
                            "allowTTL": 0
                        }
                    ]
                }
            ],
            "unprotectedApps": [
                {
                    "packageName": "com.android.chrome",
                    "reason": "Loads websites"
                }
            ],
            "unhideSystemApps": [
                "com.android.chrome"
            ]
        }        
    },
    "exceptions": []
}
