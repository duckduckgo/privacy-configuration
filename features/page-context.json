{
    "_meta": {
        "description": "Feature to collect and analyze page content for query relevance analysis. Collects text content, headings, links, and metadata from web pages."
    },
    "state": "disabled",
    "settings": {
        "maxContentLength": 950,
        "includeImages": true,
        "includeLinks": true,
        "contentSelectors": [
            "p",
            "h1",
            "h2",
            "h3",
            "article",
            "section"
        ],
        "excludeSelectors": [
            ".ad",
            ".sidebar",
            ".footer",
            ".nav",
            ".header",
            ".banner",
            ".popup"
        ]
    },
    "exceptions": []
}
